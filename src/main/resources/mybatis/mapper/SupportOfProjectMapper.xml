<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.doneasy.don.repository.project.SupportOfProjectRepository">
    <insert id="saveSupport" parameterType="com.doneasy.don.domain.project.SupportOfProject">
        insert into supportofproject values (supportofproject_seq.nextval, sysdate, 1, #{project_id});
    </insert>
    <select id="getSupport" resultType="com.doneasy.don.dto.SupportOfProjectShowDto">
        select id, member_id, project_id from supportofproject where project_id = #{id}
    </select>
</mapper>