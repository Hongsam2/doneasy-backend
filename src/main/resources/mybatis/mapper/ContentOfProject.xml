<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.doneasy.don.repository.project.ContentOfProjectRepository">
    <select id="getAll" resultType="com.doneasy.don.domain.project.ContentOfProject">
        select * from contentofproject
    </select>
    <select id="findAllByProjectId" resultType="com.doneasy.don.domain.project.ContentOfProject">
        select * from contentofproject where p_id=#{projectId}
    </select>
</mapper>

